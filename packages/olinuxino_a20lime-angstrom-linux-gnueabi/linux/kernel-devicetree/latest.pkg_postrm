	cd /boot
	for DTB in sun7i-a20-olinuxino-lime.dtb
	do
		DTB_BASE_NAME=`basename ${DTB} | awk -F "." '{print $1}'`
		DTB_SYMLINK_NAME=`echo uImage-olinuxino-a20lime | sed "s/olinuxino-a20lime/${DTB_BASE_NAME}/g"`
