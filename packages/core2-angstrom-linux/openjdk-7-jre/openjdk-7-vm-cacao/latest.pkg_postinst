        if grep -q "\-server ERROR" $D//usr/lib/jvm/java-7-openjdk/jre/lib/i386/jvm.cfg && ! grep -q "KNOWN" $D//usr/lib/jvm/java-7-openjdk/jre/lib/i386/jvm.cfg; then
